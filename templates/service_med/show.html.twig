{% extends 'admin/base_back.html.twig' %}

{% block title %}Service Médical{% endblock %}

{% block body %}
    <h1>Service Médical</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ service_med.id }}</td>
            </tr>
            <tr>
                <th>Nom du Service</th>
                <td>{{ service_med.nomService }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ service_med.descriptionMed }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_service_med_index') }}">← Retour à la liste</a>

    <a href="{{ path('app_service_med_edit', {'id': service_med.id}) }}">Modifier</a>

    {{ include('service_med/_delete_form.html.twig') }}
{% endblock %}
