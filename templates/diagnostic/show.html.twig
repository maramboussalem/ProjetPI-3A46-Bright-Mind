{% extends 'admin/base_back.html.twig' %}

{% block title %}Diagnostic{% endblock %}

{% block body %}
    <h1>Diagnostic</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ diagnostic.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ diagnostic.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ diagnostic.description }}</td>
            </tr>
            <tr>
                <th>DateDiagnostic</th>
                <td>{{ diagnostic.dateDiagnostic ? diagnostic.dateDiagnostic|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>PatientID</th>
                <td>{{ diagnostic.patientID }}</td>
            </tr>
            <tr>
                <th>MedecinID</th>
                <td>{{ diagnostic.medecinID }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_diagnostic_index') }}">back to list</a>

    <a href="{{ path('app_diagnostic_edit', {'id': diagnostic.id}) }}">edit</a>

    {{ include('diagnostic/_delete_form.html.twig') }}
{% endblock %}
