<form method="post" action="{{ path('admin_delete', {'id': utilisateur.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer votre Compte ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ utilisateur.id) }}">
    <button type="submit" class="btn btn-danger">Supprimer</button>
</form>
